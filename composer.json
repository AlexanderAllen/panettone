{
    "name": "alexanderallen/panettone",
    "type": "library",
    "description": "A lightweight PHP type generator for Open API (formerly Swagger)",
    "keywords": [
        "model",
        "entity",
        "schema",
        "dto",
        "data transfer objects",
        "open api",
        "swagger"
    ],
    "homepage": "https://github.com/AlexanderAllen/panettone",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Allen",
            "email": "14018885+AlexanderAllen@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlexanderAllen\\Panettone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexanderAllen\\Panettone\\Tests\\": "tests/"
        }
    },
    "require": {
        "psr/log": "^3.0",
        "symfony/console": "^6.0",
        "api-platform/schema-generator": "^5.2.2",
        "loophp/collection": "^7.5",
        "nikic/iter": "^2.4"
    },
    "require-dev": {
        "myclabs/php-enum": "^1.8",
        "cebe/php-openapi": "^1.7",
        "nette/neon":"^3.4",
        "nette/php-generator":"^4.1",
        "squizlabs/php_codesniffer": "^3.9",
        "phpunit/phpunit": "^10.0",
        "slevomat/coding-standard": "^8.15",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-phpunit": "^1.3",
        "cweagans/composer-patches": "^1.7",
        "nunomaduro/phpinsights": "^2.11"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "patches": {
            "api-platform/schema-generator": {
                "Prop generator fails to evalulate references": "patches/2024-3-16-propgen-ref.patch"
            }
        }
    }
}
